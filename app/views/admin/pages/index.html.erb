<%= flash[:page] %>
<div class="page_content clearfix">
  
  <h2>Edit Pages</h2>
  
<% unless @pages.blank? %>
  <ul id="events_list" class='listing'>
  <% @pages.each do |page| %>
    <li><%= link_to page.title, edit_admin_page_path(page) %></li>
  <% end %>
  </ul>
</div>  

<% else %>
  <h2>There are currently no pages here to edit</h2>
<% end %>
</div>